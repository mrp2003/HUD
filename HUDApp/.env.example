# HERE Maps API Credentials
# Copy this file to .env and fill in your actual credentials
# Get your credentials from: https://developer.here.com

# REST API credentials (for routing, geocoding)
HERE_APP_ID=your_app_id_here
HERE_API_KEY=your_api_key_here

# SDK credentials (for native lane guidance)
# Generate these from: Projects > Credentials > Generate SDK credentials
HERE_SDK_ACCESS_KEY_ID=your_sdk_access_key_id
HERE_SDK_ACCESS_KEY_SECRET=your_sdk_access_key_secret
